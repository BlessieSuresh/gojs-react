    "react": "^16.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.25.1",
    "rollup-plugin-banner": "^0.2.1",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "typescript": "^3.6.2"
